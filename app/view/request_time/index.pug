doctype html
html
  head
    title= title
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=Edge,chrome=1")
    meta(name="viewport", content="width=device-width,initial-scale=1,user-scalable=no")
    meta(name="renderer", content="webkit")
    meta(name="format-detection", content="telephone=no")
    link(rel="stylesheet" href="//cdn.staticfile.org/twitter-bootstrap/4.1.3/css/bootstrap.min.css")
    link(rel="stylesheet" href="//cdn.bootcss.com/bootstrap-table/1.12.1/bootstrap-table.min.css")
    script(src='//cdn.bootcss.com/jquery/1.12.4/jquery.min.js')
    script(src='//cdn.staticfile.org/twitter-bootstrap/4.1.3/js/bootstrap.min.js')
    script(src='//cdn.staticfile.org/jquery.form/4.2.2/jquery.form.min.js')
    script(src='//cdn.bootcss.com/jquery-sortable/0.9.13/jquery-sortable-min.js')
  body
    .container.mt-3
      for o in data
        div.mb-3
          h4= o.datetime
          p API总请求次数: #{o.total}, 平均响应时间: #{o.avg.toFixed(1)}ms
          table(id="urls-table" class="table table-striped" data-toggle="table")
            thead
              tr
                th.text-right(width=100)
                th
                th(width=100 data-sortable="true") 次数
                th(width=100 data-sortable="true") 平均请求时间(ms)
            tbody
              for u in o.requests
                tr
                  td.text-right= u.method
                  td= u.url
                  td= u.count
                  td= u.avgtimes.toFixed(1)
      script(src='https://cdn.bootcss.com/bootstrap-table/1.12.1/bootstrap-table.min.js')
